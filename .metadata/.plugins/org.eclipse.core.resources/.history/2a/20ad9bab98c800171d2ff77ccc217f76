package linkedList;


public class ListNode<E> 
{

	      E val;
	      ListNode<E> next;
	      ListNode(E x) 
	      {
	          val = x;
	          next = null;
	      }
	      
	     public void display()
	  	{
	    	 ListNode curr = this;
	    	 
	  		while(curr!=null)
	  		{
	  			if(curr.next!=null)
	  			{
	  				System.out.print(curr.val+"-->");
	  			}
	  			else
	  			{
	  				System.out.print(curr.val);
	  			}
	  			curr = curr.next;
	  		}
	  	}
}

	
