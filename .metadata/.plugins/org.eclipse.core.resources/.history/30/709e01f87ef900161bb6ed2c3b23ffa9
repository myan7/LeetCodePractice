package forLoopTest;

public class ForConditionTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		for(int i = 0;i<3;i++)
		{
			System.out.println("a");
		}
		
		int i =0;
		for(System.out.println("a");i<3;System.out.println("b"))
		{
			System.out.println("c");
			i++;
		}
	}

}
