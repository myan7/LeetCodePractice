import java.util.Arrays;


public class Combo 
{	
	int[] combo = new int[3];

	public Combo(int[] combo) 
	{
		super();
		this.combo = combo;
	}

	public boolean equals(Combo test)
	{
		boolean flag = false;
		for(int i =0; i<combo.length; i++)
		{
			if(test.combo[i] == combo[i]);
			flag = true;
		}
		return flag;
	}
	
	
	public void toString() 
	{
		int a = combo[0];
		int b = combo[1];
		int c = combo[2];
		
		System.out.println("(a, b, c)");
		return;
	}
	
	
	
	
}
